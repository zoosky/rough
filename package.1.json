{
  "name": "es6-starter",
  "version": "0.0.1",
  "description": "ES6, babel, webpack, rollup project starter.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:runscript": "node tester.js",
    "test:openbrowser": "google-chrome tester.html",
    "start:dev": "webpack --watch",
    "dev": "rollup -c -w",
    "build:webpack": "webpack --progress --hide-modules",
    "build:rollup": "rollup -c",
    "build:all": "npm run build:webpack && npm run build:rollup",
    "serve": "http-server ./ -o"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-syntax-class-properties": "^6.13.0",
    "babel-plugin-transform-class": "^0.3.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.4",
    "http-server": "^0.11.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jofftiquez/es6-starter.git"
  },
  "keywords": [
    "es5",
    "es6",
    "babel",
    "webpack",
    "rollup"
  ],
  "author": "Jofferson Ramirez Tiquez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jofftiquez/es6-starter/issues"
  },
  "homepage": "https://github.com/jofftiquez/es6-starter#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "core-js": "^2.5.4"
  }
}
